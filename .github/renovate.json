{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "github>aquaproj/aqua-renovate-config#1.8.0"
  ],
  "baseBranches": [
    "main",
    "v2"
  ],
  "labels": [
    "dependencies"
  ],
  "reviewers": [
    "CrystalMethod"
  ],
